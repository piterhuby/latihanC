#include <iostream>


using namespace std;

string firstName = "Eko Ari";
string lastName = "Nugroho";

string fullName(){
    return firstName + " " + lastName;
}

void printName(string firstName, string lastName){
    cout << " Nama saya adalah " << firstName;
}

int main()
{
   printName(firstName, lastName);
   cout << "\nNama saya adalah ,," <<fullName();
   return 0;
}
